(function() {
  'use strict';

  angular.module('{{ngModule}}')
  {{#each resources}}
  /**
  * @ngdoc service
  * @name {{../ngModule}}.{{name}}
  * @description Angular resource to fetch {{param.href}}
  **/
  .service('{{name}}', function($resource){
    return $resource('{{param.href}}'{{#if param.name}}, { {{param.name}}: '@id' }, {
      update: { method: 'PUT' }
    }{{/if}});
  })
  {{/each}}
})();